def Domination():
    print("Domination chosen")

def Teamdeathmatch():
    print("Teamdeathmatch chosen")

def Headquarters():
    print("Headquarters chosen")

def menu():
    print("press 1 for Domination")
    print("press 2 for Teamdeathmatch")
    print("press 3 for Headquarters")

    userInput = int(input("Enter 1,2,3 or 4 >>>"))

    if userInput == 1:
        Domination()
    elif userInput == 2:
        Teamdeathmatch()
    elif userInput ==3:
        Headquarters()
    else:
        print("invalid input")

menu()
